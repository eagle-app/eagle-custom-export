{
    "id": "custom-export",
    "version": "1.0.0",
    "platform": "all",
    "arch": "all",
    "name": "{{manifest.app.name}}",
    "logo": "./logo.png",
    "keywords": [],
    "dependencies": [
        "ffmpeg"
    ],
    "fallbackLanguage": "en",
    "languages": [
        "en",
        "zh_TW",
        "zh_CN",
        "ja_JP",
        "es_ES",
        "ru_RU",
        "de_DE",
        "ko_KR"
    ],
    "main": {
        "multiple": false,
        "frame": false,
        "url": "index.html",
        "vibrancy": true,
        "minWidth": 960,
        "minHeight": 600,
        "width": 1080,
        "height": 720,
        "maxWidth": 1200,
        "maxHeight": 800,
        "maximizable": false,
        "minimizable": false,
        "fullscreenable": false,
        "opacity": 0
    }
}